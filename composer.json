{
    "name": "gopos/gopos",
    "description": "GoPos - Complete Point of Sale & ERP System for Laravel",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Ahmad",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "barryvdh/laravel-dompdf": "^3.1",
        "bezhansalleh/filament-language-switch": "~4.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "filament/filament": "~4.0",
        "laravel/framework": "^12.0",
        "mpdf/mpdf": "^8.2",
        "outerweb/settings": "^1.1",
        "spatie/laravel-pdf": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Gopos\\": "src/",
            "Gopos\\Database\\Factories\\": "database/factories/",
            "Gopos\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gopos\\GoposServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
